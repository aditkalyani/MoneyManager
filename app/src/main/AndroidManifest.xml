<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phantomorion.moneymanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingleFamilySignInActivity" />
        <activity android:name=".FormActivity" />
        <activity android:name=".noInternet" />
        <activity android:name=".signInOptions" />
        <activity android:name=".BottomNavigationActivity" />
        <activity android:name=".timepass"></activity>
    </application>

</manifest>